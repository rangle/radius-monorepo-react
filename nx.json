{
  "$schema": "node_modules/nx/schemas/nx-schema.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "dev"
        ],
        "accessToken": "N2ZmNDhhYWYtOTEwZS00YjM1LTkwYmItNjBiN2U5ODk1OTU1fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "prod": [
      "!{projectRoot}/**/*.spec.tsx"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^test",
        "build"
      ],
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "defaultBase": "main",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    }
  }
}
