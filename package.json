{
  "name": "root",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "demo": "nx run demo:dev",
    "build:demo": "nx run demo:build",
    "run-many": "nx run-many --exclude=demo",
    "build": "yarn run run-many --target=build",
    "build:affected": "yarn run build --affected",
    "build:force": "yarn run build --skip-nx-cache",
    "test": "yarn run run-many --target=test",
    "test:affected": "yarn run test --affected",
    "test:watch": "nx run @rangle/radius-react-core-components:test-watch",
    "publish": "lerna publish from-package --yes",
    "publish:next": "lerna publish from-package --canary --preid next --yes",
    "local-publish": "lerna publish --no-git-tag-version --no-push --registry http://localhost:4873",
    "local-unpublish": "lerna exec -- npm unpublish --registry=http://localhost:4873 \"\\${LERNA_PACKAGE_NAME}@\\$(npm view \\$LERNA_PACKAGE_NAME version)\"",
    "local-unpublish-all": "lerna exec -- npm unpublish -f --registry=\"http://localhost:4873/\" \"\\${LERNA_PACKAGE_NAME}\"",
    "lint": "NODE_ENV=test ./node_modules/.bin/eslint -c ./.eslintrc . --report-unused-disable-directives --ignore-path ./.eslintignore",
    "ls-lint": "ls-lint",
    "lint:ci": "yarn lint && yarn ls-lint",
    "clean": "lerna run clean && rm -rf node_modules",
    "graph": "nx graph",
    "storybook": "yarn run build && sb dev -p 6006",
    "build-storybook": "yarn run build && sb build",
    "prepare": "husky install"
  },
  "workspaces": [
    "apps/*",
    "library/*",
    "shared/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@emotion/babel-plugin": "11.10.5",
    "@ls-lint/ls-lint": "^1.0.0",
    "@nrwl/jest": "^15.9.2",
    "@nrwl/nx-cloud": "^15.3.5",
    "@nrwl/react": "^15.9.2",
    "@nrwl/storybook": "^15.9.2",
    "@nrwl/web": "^15.9.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@storybook/addon-actions": "^7.0.10",
    "@storybook/addon-docs": "^7.0.10",
    "@storybook/addon-essentials": "^7.0.10",
    "@storybook/addon-interactions": "^7.0.10",
    "@storybook/addon-links": "^7.0.10",
    "@storybook/addon-mdx-gfm": "^7.0.10",
    "@storybook/cli": "^7.0.10",
    "@storybook/react": "^7.0.10",
    "@storybook/react-webpack5": "^7.0.10",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "29.4.4",
    "@types/jest-axe": "^3.5.5",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "add": "^2.0.6",
    "autoprefixer": "10.4.13",
    "aws-cdk-lib": "2.62.2",
    "babel-jest": "29.4.3",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.14.0",
    "constructs": "10.1.156",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.4.3",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "29.4.3",
    "lerna": "^6.0.1",
    "nx": "^15.9.2",
    "postcss": "8.4.19",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "18.2.0",
    "rollup": "^3.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sst": "^2.0.38",
    "storybook-addon-pseudo-states": "^2.0.0-next.0",
